{
    "name": "@paychex/platform-browser",
    "version": "1.0.1",
    "description": "Provides agnostic JavaScript functionality for use in all browser-based applications.",
    "main": "dist/cjs/index.js",
    "module": "dist/esm/index.js",
    "files": [
        "dist",
        "docs"
    ],
    "keywords": [
        "paychex",
        "browser",
        "cross-origin",
        "stores"
    ],
    "license": "Apache-2.0",
    "author": "Paychex",
    "contributors": [
        {
            "name": "Dan Barnes",
            "email": "dbarnes1@paychex.com"
        }
    ],
    "devDependencies": {
        "@babel/core": "^7.13.1",
        "@babel/plugin-proposal-class-properties": "^7.13.0",
        "@babel/preset-env": "^7.13.5",
        "@rollup/plugin-babel": "^5.3.0",
        "@rollup/plugin-commonjs": "^17.1.0",
        "@rollup/plugin-node-resolve": "^11.2.0",
        "c8": "^7.3.5",
        "docdash": "^1.2.0",
        "expect": "^26.0.1",
        "jsdoc": "^3.6.4",
        "mocha": "^8.3.0",
        "rimraf": "^3.0.2",
        "rollup": "^2.39.1",
        "rollup-plugin-node-polyfills": "^0.2.1",
        "rollup-plugin-terser": "^7.0.2"
    },
    "scripts": {
        "test": "mocha --recursive",
        "coverage": "c8 --exclude=types --exclude=test --reporter=html mocha --recursive",
        "docs": "jsdoc -c jsdoc.json",
        "build": "rollup -c",
        "clean": "rimraf dist docs coverage",
        "preversion": "npm run clean && npm run docs && npm run build",
        "prepublish": "c8 --exclude=types --exclude=test --reporter=html --check-coverage --branches 100 --functions 100 --lines 100 --statements 100 mocha --recursive"
    },
    "dependencies": {
        "@paychex/core": "^2.0.4"
    }
}
